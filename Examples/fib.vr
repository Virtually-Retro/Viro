<VirtuallyRetro-File>
<source>;Fibonacci Sequence
_start:
	sys.ignorelocal
	
	lda const_highRes_mode
	out screen.mode
		
	lda #60
	stc

loop:
	lda cu
	sub c
	out text.cursor.y
	lda #0
	out text.cursor.x

	lda av
	add bv
	sta r0
	lda bv
	sta av
	lda r0
	sta bv

	out text.number
	lda #32
	out

	lp loop

	brk

;Variables
av:
	data 0
bv:
	data 1
cu:
	data 60</source>
