<VirtuallyRetro-File>
<source>_start:
	lda const_highres_mode
	out screen.mode

loopstart:

	lda #0
	sta av
	
	lda false
	out screen.auto_refresh

	lda #100
	stc
outerloop:
	push c
	
	gc const_gLineToReset
	
	screen.clear

	lda #640
	stc
loop:
	lda #2
	mul maths.pi
	mul fv
	mul c
	add bv
	sin
	mul av
	add #240
	sta y
	
	ld c
	ld y
	gc const_gLineTo
	
	lp loop
	
	screen.refresh
	
	lda av
	inc
	sta av
	
	pop c
	lp outerloop
	
	lda fv
	sub #0.001
	sta fv
	
	
jp loopstart
	
	brk
	
y:
	data 0
av:
	data 1
fv:
	data 0.01
bv:
	data 0</source>
<mem>1024</mem>
<row>18</row>
<col>1</col>
<hash></hash>
<scroll>0</scroll>
<autorun>false</autorun>
<exmode>false</exmode>
<fullscreen>false</fullscreen>
