<source>_start:

	jsr setup
	
mainLoop:

	in Key.Code
	jsrnz keyTest
	
	jsr drawText
	jsr drawLines
 
	jp mainLoop
	
keyTest:

	in Key.Code
	cmp const_KeyEnd
	jnz endKeyTest
	brk
	
endKeyTest:
	in
	rts
	
drawlines:
	push a
	
	lda #150
	rnd
	pds a
	lda #220
	rnd
	pds a
	lda #150
	rnd
	pds a
	lda #220
	rnd
