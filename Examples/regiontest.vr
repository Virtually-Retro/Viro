drawtext:
	push a
	
	lda #5
	rnd
	jsrz space
	jsrnz char
		
	pop a
	rts


space:
	push a
	
	lda #32
	out
	
	pop a
	rts
	
char:
	push a
	
	lda #63
	rnd
	out color.foreground
	lda #94
	rnd
	add #32
	out
	
	pop a
	rts 


setup:
	push a

	lda #0
	out color.background
	_cls
	
	pds #9
	pds #9
	pds #152
	pds #222
	call const_graphics, const_gRect		

	pds #169
	pds #9
	pds #145
	pds #217
