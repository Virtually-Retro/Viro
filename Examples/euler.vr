<VirtuallyRetro-File>
<source>;Euler maths test program
_start:
	sys.ignorelocal
	
	lda #999
	stc

loop:

	lda c
	mod #3
	cmp #0
	jsrz addnumber
	jz skip

	lda c
	mod #5
	cmp #0
	jsrz addnumber

	skip:

	lp loop

	lda r0
	out text.print

	brk

addnumber:
	push a
	lda r0
	add c
	sta r0
	pop a
	rts
</source>
<mem>1024</mem>
<row>25</row>
<col>15</col>
<hash></hash>
<scroll>5</scroll>
<autorun>false</autorun>
<exmode>false</exmode>
<fullscreen>false</fullscreen>
