<VirtuallyRetro-File>
<source>_start:

	sys.ignorelocal

	lda #bindata
	add #7
	sta mp

	lda #8
	stc
	
loop:
	lda num
	and #1
	sta [mp]
	lda mp
	dec
	sta mp
	lda num
	shr
	sta num

	lp loop

	lda #8
	stc
drawLoop:
	mb.read #binData
	out text.number
	lp drawLoop

	brk

num:
	data 221

mp:
	data 0

bindata:
	mb 8</source>
<mem>1024</mem>
<row>40</row>
<col>3</col>
<hash></hash>
<scroll>9</scroll>
<autorun>false</autorun>
<exmode>false</exmode>
<fullscreen>false</fullscreen>
