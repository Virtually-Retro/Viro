<VirtuallyRetro-File>
<source>_start:
	.ignorelocal
	
	lda #0.05
	out sys.performance

	lda "test.txt" ;Opens file
	out file.io
		
mainloop:
	in file.io ;Reads one byte from the file into A
	jz exit
	out
		
	jp mainloop
	
exit:
	brk

;Resources
	.link "test.txt"</source>
<mem>1024</mem>
<row>9</row>
<col>4</col>
<hash></hash>
<scroll>0</scroll>
<autorun>false</autorun>
<exmode>false</exmode>
<fullscreen>false</fullscreen>
